{
   "compilerOptions": {
      "lib": ["es2023"],
      "module": "node16",
      "target": "es2022",
      "baseUrl": ".",
      "resolveJsonModule": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node16"
   },
   "include": ["src", "*.d.ts"],
   "exclude": ["node_modules"]
}
